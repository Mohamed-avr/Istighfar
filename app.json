{
  "expo": {
    "name": "Istighfar",
    "slug": "Istighfar",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.mohamed.istighfar"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/ReadexPro.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Readex Pro",
                "fontDefinitions": [
                  {
                    "path": "./src/assets/fonts/ReadexPro.ttf",
                    "weight": 400
                  },
                  {
                    "path": "./src/assets/fonts/ReadexPro.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./src/assets/fonts/ReadexPro.ttf",
                    "weight": 600
                  },
                  {
                    "path": "./src/assets/fonts/ReadexPro.ttf",
                    "weight": 800
                  }
                ]
              }
            ]
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d8ecea0c-67fd-42ba-bb5a-ad1e6c0ed8a7"
      }
    }
  }
}
